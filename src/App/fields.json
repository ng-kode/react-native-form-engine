    "template": "Text",
    "customize": {
      "title": "客戶資料",
      "type": "h1"
    }
  },
  {
    "path": "hasAmericanPassport",
    "template": "RadioField",
    "customize": {
      "label": "閣下是否持有美國護照或於美國居住?",
      "options": [
        {"label": "是", "value": true},
        {"label": "否", "value": false}
