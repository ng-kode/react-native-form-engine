  {
    "path": "educationLevel",
    "template": "RadioField",
    "customize": {
      "label": "教育程度",
      "options": [
        {"label": "小學或以下", "value": 1001},
        {"label": "中學", "value": 1002},
        {"label": "大專/大學或以上", "value": 1003},
        {"label": "其他(請列明)", "value": 1004}
      ]
    }
  }
